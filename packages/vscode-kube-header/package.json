{
  "name": "vscode-kube-header",
  "displayName": "kube Header",
  "description": "kube Header for VSCode",
  "version": "1.2.13",
  "publisher": "kube",
  "license": "MIT",
  "icon": "kube.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kube/header"
  },
  "bugs": {
    "url": "https://github.com/kube/header/issues"
  },
  "main": "./dist/extension",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "dev": "webpack --mode development --devtool source-map",
    "build": "webpack --mode production",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "kube-header": "^1.2.13"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/webpack": "^4.41.21",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "contributes": {
    "commands": [
      {
        "command": "kube.insertHeader",
        "title": "Insert Kube Header",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "kube.insertHeader",
        "key": "ctrl+alt+k",
        "mac": "cmd+alt+k",
        "when": "editorTextFocus"
      }
    ]
  }
}
